{
  "name": "@repo/db",
  "version": "0.0.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "db:studio": "dotenv -e ../../.env prisma studio",
    "db:generate": "prisma generate",
    "db:migrate": "dotenv -e ../../.env prisma migrate dev",
