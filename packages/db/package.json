    "db:push": "dotenv -e ../../.env prisma db push --skip-generate",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "zod": "^3.23.8",
    "zod-prisma": "^0.5.4",
    "prisma": "^5.4.2"
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "@types/node": "20.8.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "@repo/eslint-config": "workspace:*",
    "@repo/tsconfig": "workspace:*"
  }
}
